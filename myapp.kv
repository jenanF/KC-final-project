<ConvertorScreen>:
  name: "convert"
  size_hint: 0.6,0.7
  pos_hint: {'center_x': 0.5, 'center_y' : 0.5}
  
  
  GridLayout:
    cols:1
    spacing: 10,20

    GridLayout:
      cols:3
      size_hint_y: 0.5

      Button:
        background_color: 0,0,0,0
        on_release:
          app.root.current= "ukk"
          root.manager.transition.direction = "right"
        Image:
          source: "backgo.png"
          center_x: self.parent.center_x
          center_y: self.parent.center_y
      Button:
        background_color: 0,0,0,0
      Button:
        background_color: 0,0,0,0


    Image:
      source: "exchangee.png"
    
    GridLayout:
      cols:2
      spacing: 10,10

      Label:
        text: "from:"
        font_size: 30
      TextInput:
        name: "from_"
        id: fromss
        size_hint: 0.2,0.2

      Label:
        text: "to:"
        font_size: 30
      TextInput:
        name: "to_"
        id: tooo
        size_hint: 0.2,0.2 

      Label:
        text: "amount:"
        font_size: 30
        size_hint: 0.3,1

      TextInput:
        name: "amount"
        id: amounts
        size_hint: 0.7,0.2 

    Button:
      text: "convert"
      size_hint: 0.1,0.5
      on_press: root.convert()
      
    Label:
      name: "resultt"
      id: res
      text: "result:"
      font_size: 30


